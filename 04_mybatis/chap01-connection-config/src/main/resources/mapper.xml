<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="mapper">  <!--매퍼 찾을 때 이름으로 찾으려고-->
    <select id="selectSysDate" resultType="java.util.Date">
        SELECT
            CURDATE()
        FROM DUAL
    </select>
</mapper>